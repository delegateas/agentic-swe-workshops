### Step 3: Tool Execution Functions

#!csharp

// Tool execution - kobler tool calls til vores implementationer
string ExecuteTool(string toolName, JsonElement input)
{
    return toolName switch
    {
        "search_chargers" => SearchChargers(input),
        "get_charger_status" => GetChargerStatusJson(input),
        "calculate_charging_cost" => CalculateCostJson(input),
        _ => throw new ArgumentException($"Unknown tool: {toolName}")
    };
}

string SearchChargers(JsonElement input)
{
    var location = input.GetProperty("location").GetString()!.ToLower();
    var minPower = input.TryGetProperty("min_power_kw", out var mp) ? mp.GetInt32() : 0;

    var results = chargers.Values
        .Where(c => c.Location.ToLower().Contains(location) || location.Contains("kÃ¸ben"))
        .Where(c => c.PowerKw >= minPower)
        .Select(c => new { c.Id, c.Name, c.Location, c.PowerKw, c.Type })
        .ToList();

    return JsonSerializer.Serialize(results);
}

string GetChargerStatusJson(JsonElement input)
{
    var chargerId = input.GetProperty("charger_id").GetString()!;

    if (!chargers.TryGetValue(chargerId, out var charger))
        return JsonSerializer.Serialize(new { error = $"Unknown charger: {chargerId}" });

    var hasSession = activeSessions.TryGetValue(chargerId, out var session);

    var status = new
    {
        charger_id = chargerId,
        name = charger.Name,
        available = !hasSession,
        power_kw = charger.PowerKw,
        current_session = hasSession ? session.vehicleId : null,
        energy_delivered_kwh = hasSession ? session.kwh : (double?)null
    };

    return JsonSerializer.Serialize(status);
}

string CalculateCostJson(JsonElement input)
{
    var kwh = input.GetProperty("kwh").GetDouble();
    var hour = input.GetProperty("hour").GetInt32();

    decimal pricePerKwh = hour switch
    {
        >= 0 and < 6 => 1.50m,
        >= 6 and < 17 => 2.50m,
        >= 17 and < 21 => 4.00m,
        >= 21 and <= 23 => 2.50m,
        _ => 2.50m
    };

    var tariff = hour switch
    {
        >= 0 and < 6 => "off-peak",
        >= 17 and < 21 => "peak",
        _ => "normal"
    };

    return JsonSerializer.Serialize(new
    {
        kwh,
        hour,
        tariff,
        price_per_kwh = pricePerKwh,
        total_cost_dkk = (decimal)kwh * pricePerKwh
    });
}

